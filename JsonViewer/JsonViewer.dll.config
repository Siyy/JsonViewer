<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="jsonViewer" type="EPocalipse.Json.Viewer.ViewerConfiguration, JsonViewer"/>
  </configSections>
  <jsonViewer>
    <plugins>
      <add key="default" value="EPocalipse.Json.Viewer.JsonObjectVisualizer, JsonViewer"/>
