      <add key="CustomDate" value="EPocalipse.Json.Viewer.CustomDate, JsonViewer"/>
      <add key="Grid" value="EPocalipse.Json.Viewer.GridVisualizer, JsonViewer"/>
      <!--<add key="Sample" value="EPocalipse.Json.Viewer.Sample, JsonViewer"/>-->
    </plugins>
  </jsonViewer>
